FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1750;

boundaryField
{
    wall_mould
    {
        type            fixedGradient;
        gradient        uniform -85000;
    }
    wall_sen
    {
        type            zeroGradient;
    }
    wall_atm
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1750;
    }
    atm
    {
        type            zeroGradient;
    }
}


