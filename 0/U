FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 -0.016666667);

boundaryField
{
    wall_mould
    {
        type            noSlip;
    }
    wall_atm
    {
        type            noSlip;
    }
    wall_sen
    {
        type            noSlip;
    }
    outlet
    {
        type            flowRateOutletVelocity;
        massFlowRate    
        {
            type            constant;
            value           32.76;
        }
        rhoOutlet       7020;
        value           uniform (0 0 -0.016666667);
    }
    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            constant;
            value           32.76;
        }
        rhoInlet        7020;
        value           uniform (0 0 1.3);
    }
    atm
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
}


