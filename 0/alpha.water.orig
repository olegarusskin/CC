FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphawater;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    wall_sen
    {
        type            zeroGradient;
    }
    
    wall_mould
    {
        type            zeroGradient;
    }

    wall_atm
    {
        type            zeroGradient;
    }
    
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }

    outlet
    {
        type            zeroGradient;
    }

    atm
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

}

