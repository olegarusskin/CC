FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphawater;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    wall_air
    {
        type            zeroGradient;
    }
    
    wall_liquid
    {
        type            zeroGradient;
    }

    wall_solid
    {
        type            zeroGradient;
    }
    
    wall_cold
    {
       type            zeroGradient;
    }

}

