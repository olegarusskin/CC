FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    wall_air
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    wall_liquid
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    wall_solid
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    wall_cold
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

}

