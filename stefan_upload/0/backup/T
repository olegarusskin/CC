FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 277.35;

boundaryField
{
    wall_liquid
    {
        type            zeroGradient;
    }
    wall_solid
    {
        type            zeroGradient;
    }
    wall_air
    {
        type            zeroGradient;
    }
    wall_cold
    {
        type            fixedValue;
        value           uniform 263.15;
    }
}


