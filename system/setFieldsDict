FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
    volScalarFieldValue T           1750
);


regions
(
    zoneToCell
    {
        zone        atmosphere;

        fieldValues
        (
            volScalarFieldValue alpha.water 0
            //volScalarFieldValue T           300
        );
    }
);
